                            <div class="footer_title">Contacts</div>
                            <ul class="contact_list">
                                <li>01555555555555</li>
                                <li>email@gmail.com</li>
                                <li>contact@gmail.com</li>
                            </ul>
                        </div>
                    </div>

                    <!-- Footer Locations -->
                    <div class="col-lg-3 footer_col">
                        <div class="footer_location">
