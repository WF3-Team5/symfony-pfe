                                    <div class="icon_box_text">
                                        <p>suivi</p>
                                    </div>
                                </div>


                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- CTA 2 -->

            <div class="cta_2">
                <div class="container">
                    <div class="row row-lg-eq-height">
                        <div class="col-lg-9 cta_2_col">
                            <div class="cta_2_content">
